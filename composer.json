{
  "name": "panamax/panamax",
  "type": "library",
  "description": "Create psr-11 container agnostic applications",
  "homepage": "https://github.com/spider-mane/panamax",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com",
      "homepage": "https://github.com/spider-mane",
      "role": "Developer"
    }
  ],
  "keywords": [
    "panamax",
    "webtheory"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4||^8.0",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "captainhook/captainhook": "^5.10",
    "fakerphp/faker": "^1.16",
    "friendsofphp/php-cs-fixer": "^3.3",
    "league/container": "^4.2",
    "mockery/mockery": "^1.4",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/phpstan": "^0.12.83",
    "phpunit/phpunit": "^9.5",
    "psy/psysh": "^0.10.7",
    "spatie/phpunit-watcher": "^1.12",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/var-dumper": "^5.4",
    "vimeo/psalm": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "WebTheory\\Panamax\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "env": "cp -n .env.example .env && bin/build-paths",
    "test": "vendor/bin/phpunit",
    "watch": "vendor/bin/phpunit-watcher",
    "check": "vendor/bin/phpcs",
    "fix": "vendor/bin/phpcbf",
    "style": "vendor/bin/php-cs-fixer fix --verbose",
    "inspect": "vendor/bin/psalm",
    "shell": "vendor/bin/psysh",
    "collect": "vendor/bin/var-dump-server",
    "hooks": "vendor/bin/captainhook",
    "build": [
      "@fix",
      "@check",
      "@inspect",
      "@test"
    ]
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "extra": {
    "build-paths": {
      "build": {
        "phpunit": {},
        "php-cs-fixer": {}
      },
      "logs": {}
    }
  }
}
